# Node Environment
NODE_ENV=development

# API Configuration
PORT=3000
API_PREFIX=api

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/dashboard_macro

# Redis
REDIS_URL=redis://localhost:6379

# External APIs
NEWS_API_KEY=your_newsapi_key_here
ALPHAVANTAGE_KEY=your_alphavantage_key_here
IEX_CLOUD_KEY=your_iex_key_here

# Scheduler
ENABLE_SCHEDULER=true

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Logging
LOG_LEVEL=debug